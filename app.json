{
  "expo": {
    "name": "tahfidz-expo",
    "slug": "tahfidz-expo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.tahfidz.expo",
      "softwareKeyboardLayoutMode": "pan"
    },
    "updates": {
      "enabled": false
    },
    "assetBundlePatterns": [
      "**/*",
      "assets/dist/*"
    ],
    "plugins": [],
    "platforms": [
      "android"
    ],
    "extra": {
      "eas": {
        "projectId": "2518e8df-fc6d-4f5b-9c24-81afba5d83a2"
      }
    }
  }
}